{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flask: LLMOps",
      "module": "flask",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "FLASK_APP": "${cwd}/app/http/app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "1"
      },
      "args": ["run"],
      "jinja": true,
      "console": "integratedTerminal"
    }
  ]
}
