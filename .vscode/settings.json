{
  "cSpell.words": [
    "apidocs",
    "dataclass",
    "dotenv",
    "flasgger",
    "llmops",
    "Redprint",
    "sqla",
    "uiversion"
  ],
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/*.pyd": true,
    "**/__pycache__/**": true,
    ".pytest_cache": true,
    ".idea": true
  },
  "python.autoComplete.extraPaths": [
    "${workspaceFolder}/.venv/lib/python3.12/site-packages/"
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python3.12",
  "python-envs.pythonProjects": [],
  "python.analysis.autoImportCompletions": true
}
